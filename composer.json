{
    "name": "eivindarvesen/b3",
    "description": "BlaBlaBlog - a lightweight PHP Markdown Blog System mased on Lumen",
    "keywords": [
        "php",
        "bootstrap",
        "markdown",
        "blog",
        "web"
    ],
    "license": "BSD-3-Clause",
    "type": "project",
    "language": "php",
    "authors": [
        {
            "name": "Eivind Arvesen",
            "email": "eivind.arvesen@gmail.com"
        }
    ],
    "require": {
        "laravel/lumen-framework": "5.0.*",
        "vlucas/phpdotenv": "~1.0",
        "daylerees/kurenai": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
